# 开发环境配置

# 开发服务器配置
VITE_PORT=3889
VITE_BASE_URL="/"

# API配置
VITE_API_TYPE="openai"  # openai, azure, custom
VITE_API_ENDPOINT="https://api.openai.com/v1"
VITE_API_KEY=""
VITE_DEFAULT_MODEL="gpt-3.5-turbo"
VITE_DEFAULT_TEMPERATURE=0.7

# WPS开发配置
VITE_WPS_DEV=true
VITE_WPS_DEBUG=true

# 任务窗格配置
VITE_TASKPANE_WIDTH=400
VITE_TASKPANE_POSITION="right"  # left, right
VITE_TASKPANE_DEFAULT_VISIBLE=true

# 界面配置
VITE_ENABLE_DARK_MODE=true
VITE_FONT_SIZE="14px"
VITE_PRIMARY_COLOR="#2563eb"

# 调试配置
VITE_DEBUG=true
VITE_DEBUG_LEVEL="info"  # debug, info, warn, error
VITE_DEBUG_TO_CONSOLE=true

# 缓存配置
VITE_ENABLE_CACHE=true
VITE_CACHE_MAX_AGE=3600
VITE_STORAGE_PREFIX="wps_ai_"

# 性能配置
VITE_POLL_INTERVAL=1000
VITE_MAX_HISTORY_LENGTH=100

# 功能开关
VITE_FEATURE_SETTINGS=true
VITE_FEATURE_CONTEXT=true
VITE_FEATURE_HISTORY=true
